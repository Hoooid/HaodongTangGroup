@article{ WOS:000504304100003,
Author = {Zhang, Nan and Tang, Haodong and Shi, Kanming and Wang, Weigao and Deng,
   Weiqiao and Xu, Bing and Wang, Kai and Sun, Xiao Wei},
Title = {High-performance all-solution-processed quantum dot
   near-infrared-to-visible upconversion devices for harvesting
   photogenerated electrons},
Journal = {APPLIED PHYSICS LETTERS},
Year = {2019},
Volume = {115},
Number = {22},
Month = {NOV 25},
Abstract = {All-solution-processed quantum dot (QD) upconversion devices were
   realized by constructing a colloidal PbS QD near-infrared (NIR similar
   to 1 mu m)-sensitive layer on the cathode side of a red CdSe quantum dot
   light-emitting diode (QLED). Different from conventional upconversion
   devices, we utilized the photogenerated electrons rather than the holes
   created in the NIR-sensitive layer to inject into the QLED for visible
   light emission. Consequently, we obtained high-performance devices with
   a maximum photon-to-photon conversion efficiency of 3.35\% and a
   luminance on-off ratio of approximately 8 x 10(3). The results indicate
   that the device structure could be used to realize low-cost and portable
   high-performance QD-based upconversion devices via solution-processing.

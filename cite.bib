@article{ WOS:000623061800116,
Author = {Liu, Sisi and Xiong, Kao and Wang, Kang and Liang, Guijie and Li,
   Ming-Yu and Tang, Haodong and Yang, Xiaokun and Huang, Zhen and Lian,
   Linyuan and Tan, Manlin and Wang, Kai and Gao, Liang and Song, Haisheng
   and Zhang, Daoli and Gao, Jianbo and Lan, Xinzheng and Tang, Jiang and
   Zhang, Jianbing},
Title = {Efficiently Passivated PbSe Quantum Dot Solids for Infrared
   Photovoltaics},
Journal = {ACS NANO},
Year = {2021},

@article{ WOS:000595204600001,
Author = {Liu, Sisi and Zhang, Chongjian and Li, Shuangyuan and Xia, Yong and
   Wang, Kang and Xiong, Kao and Tang, Haodong and Lian, Linyuan and Liu,
   Xinxing and Li, Ming-Yu and Tan, Manlin and Gao, Liang and Niu, Guangda
   and Liu, Huan and Song, Haisheng and Zhang, Daoli and Gao, Jianbo and
   Lan, Xinzheng and Wang, Kai and Sun, Xiao Wei and Yang, Ye and Tang,
   Jiang and Zhang, Jianbing},
Title = {Efficient Infrared Solar Cells Employing Quantum Dot Solids with Strong
   Inter-Dot Coupling and Efficient Passivation},
Journal = {ADVANCED FUNCTIONAL MATERIALS},
Year = {2021},
Volume = {31},
Number = {9},
Month = {2021 FEB},
Abstract = {Lead chalcogenide quantum dot (QD) infrared (IR) solar cells are
   promising devices for breaking through the theoretical efficiency limit
   of single-junction solar cells by harvesting the low-energy IR photons
   that cannot be utilized by common devices. However, the device
   performance of QD IR photovoltaic is limited by the restrictive relation
   between open-circuit voltages (V-OC) and short circuit current densities
   (J(SC)), caused by the contradiction between surface passivation and
   electronic coupling of QD solids. Here, a strategy is developed to
   decouple this restriction via epitaxially coating a thin PbS shell over
   the PbSe QDs (PbSe/PbS QDs) combined with in situ halide passivation.
   The strong electronic coupling from the PbSe core gives rise to
   significant carrier delocalization, which guarantees effective carrier
   transport. Benefited from the protection of PbS shell and in situ halide
   passivation, excellent trap-state control of QDs is eventually achieved
   after the ligand exchange. By a fine control of the PbS shell thickness,
   outstanding IR J(SC) of 6.38 mA cm(-2) and IR V-OC of 0.347 V are
   simultaneously achieved under the 1100 nm-filtered solar illumination,
   providing a new route to unfreeze the trade-off between V-OC and J(SC)
   limited by the photoactive layer with a given bandgap.},
